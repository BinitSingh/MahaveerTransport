                    style="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/txt_tds_above"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edt_tds_above"
                    android:layout_width="@dimen/dimen_64dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginHorizontal="@dimen/dimen_8dp"
                    android:layout_marginTop="@dimen/dimen_4dp"
                    android:layout_toEndOf="@+id/txt_tds_above"
                    android:autofillHints=""
                    android:background="@drawable/edt_background"
                    android:ems="10"
                    android:inputType="text"
                    tools:ignore="LabelFor" />

                <TextView
                    android:id="@+id/txt_after_limit"
                    style="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/edt_tds_above"
                    android:text="@string/txt_after_limit"
                    android:textColor="@color/black" />

            </RelativeLayout>

            <TextView
                android:id="@+id/txt_exemption_upto"
                style="@style/TextAppearance.MaterialComponents.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/txt_exemption_upto"
                android:textColor="@color/black" />

            <EditText
                android:id="@+id/edt_exemption_upto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_4dp"
                android:autofillHints=""
                android:background="@drawable/edt_background"
                android:ems="10"
                android:inputType="text"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/txt_tds_declaration"
                style="@style/TextAppearance.MaterialComponents.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/txt_tds_declaration"
                android:textColor="@color/black" />

            <EditText
                android:id="@+id/edt_tds_declaration"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_4dp"
                android:autofillHints=""
                android:background="@drawable/edt_background"
                android:ems="10"
                android:inputType="text"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/txt_effective_date"
                style="@style/TextAppearance.MaterialComponents.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_16dp"
                android:text="@string/txt_effective_date"
                android:textColor="@color/black" />

            <DatePicker
                android:id="@+id/effective_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_4dp"
                android:autofillHints=""
                android:background="@drawable/edt_background"
                android:ems="10"
                tools:ignore="LabelFor" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
